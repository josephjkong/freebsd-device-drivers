static void
foo_callback(void *callbackarg, bus_dma_segment_t *segs, int nseg, int error)
{
	struct foo_softc *sc = callbackarg;
	int i;

	if (error)
		return;

	sc->sg_num = nseg;
	for (i = 0; i < nseg; i++)
		sc->sg_addr[i] = segs[i].ds_addr;
}
